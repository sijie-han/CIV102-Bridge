    "# Area calculation\n",
    "A1  = 100 * 1.27\n",
    "A2 = 1.27* 6.27\n",
    "A3 = 1.27*(75-1.27*2)\n",
    "A4 = 1.27*80\n",
    "\n",
    "# yb\n",
    "y1 = 75+ 1.27/2\n",
    "y2 = 75 - 1.27/2\n",
    "y3 = 75/2\n",
    "y4 = 1.27/2\n",
    "# y_bar\n",
    "y_bar = (A1*y1 + 2*A2*y2 + 2*A3*y3 + A4*y4)/(A1+A2+A3+A4)\n"
   ]
